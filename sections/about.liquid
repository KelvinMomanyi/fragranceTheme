  /*About*/
      .about-1 .container {
        display: grid;
        gap: 45px;
      }
      
      .about-1 .wrapper,
      .about-banner { padding-inline-start: 45px; }
      
      .about .section-title { margin-block-end: 35px; }
      
      .about-1 .section-text {
        font-size: var(--fs-6);
        color: var(--white_a70);
        font-weight: var(--fw-300);
        line-height: 1.7;
        margin-block-end: 35px;
      }
      
      .about-img{
        width: 200px;
        height: 200px;
      }
      .about-1 .wrapper::before {
        top: 10px;
        left: 0;
        background-image: url("../images/about-quote.svg");
        background-repeat: no-repeat;
        background-size: contain;
        width: 20px;
        height: 20px;
      }
      
      .about-1 .section-text .em {
        color: var(--white);
        font-weight: var(--fw-400);
        text-decoration: underline;
      }
      
      .about-banner { position: relative; }
      
      .about-1 .shape-1 {
        display: block;
        position: absolute;
        bottom: 10%;
        left: 0;
        z-index: 1;
        width: 100px;
      }
      
      
      
      
      
